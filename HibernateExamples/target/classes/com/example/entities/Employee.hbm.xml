<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.technicalkeeda.entities.Employee" table="trn_employee"
		catalog="technicalkeeda">
		<id name="empId" type="java.lang.Long">
			<column name="emp_id" />
			<generator class="identity" />
		</id>
		<property name="firstName" type="string">
			<column name="first_name" length="20" not-null="true" />
		</property>
		<property name="lastName" type="string">
			<column name="last_name" length="20" not-null="true" />
		</property>
	</class>
</hibernate-mapping>